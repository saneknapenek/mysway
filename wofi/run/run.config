#МОЙ КОНФИГ
# ширина
width=320
# высота
height=800

# скрыть подменю
no_actions=true
# скрыть полосу прокрутки
hide_scroll=true

prompt=
# терминал по умалчанию
term=alacritty

______________________________________________________________________________


#ОСНОВНОЕ

#включает поиск без учета регистра
insensitive=true

filter_rate=1 #скорость обновления результатов в милисекундах
#sort_order=ORDER порядок сортировки, default and alphabetical (по алфавиту)
#matching=contains или fuzzy режим сопоставления
dynamic_lines=false
exec_search=true

#x=0
#y=0

columns=1
#lines=1 #Задает высоту в количестве строк вместо пикселей
hide_search=false
gtk_dark=true
orientation=vertical
allow_markup=false
allow_images=false

layer=overlay
normal_window=false
location=1

line_wrap=word
global_coords=false
